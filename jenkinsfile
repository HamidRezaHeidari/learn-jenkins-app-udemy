pipeline {
    agent any

    stages {
        stage('Main') {
            steps {
                sh "echo 'Hello World'"
                sh '''
                    echo 'Hello' 
                    mkdir -p ner_dir
                '''
            }
        }
        stage("Test"){
            steps{
                echo 'Test Successfuly ...'
            }
        }
    }
    post{
        success{
            echo "success"
        }
    }
}
